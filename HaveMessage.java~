import java.util.*;
import java.io.*;

public class HaveMessage extends MessageType
{
	//private static byte have 	 = 4;
	
	private int payload;
	
	public HaveMessage(int length, byte type, int payload)
	{	
		super(length,type);
		this.payload = payload;
	}
	
	public int getPayload()
	{
		return this.payload;
	}

}
